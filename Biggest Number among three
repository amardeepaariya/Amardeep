import java.util.Scanner;
   class BiggestT
   {
	   public static void main(String[] args)
	   {
		   Scanner sc=new Scanner(System.in);
		   System.out.println("Enter any Three Integers");
		   int a,b,c;
		    a=sc.nextInt();
		    b=sc.nextInt();
		    c=sc.nextInt();
		   int big;
		   
		   big=(a>b&&a>c)?a:(b>c)?b:c;
		   
		   System.out.println("Biggest No. = "+big);
	   }
   }
