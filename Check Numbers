import java.util.Scanner;
 class CheckNo
 {
	 public static void main(String[] args)
	 {
		Scanner sc=new Scanner(System.in);
        System.out.println("Enter the No.");
        int n=sc.nextInt();
        if(n>=0)
        System.out.println("The No. is +ve");
        else
			System.out.println("The No. is -ve");
			
	 }
 }
