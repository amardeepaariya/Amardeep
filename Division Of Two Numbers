import java.util.Scanner;
public class Division
{
	public static void main(String []args)
	{
		Scanner user=new Scanner(System.in);
		System.out.println("Enter two numbers");
		int a=user.nextInt();
		int b=user.nextInt();
		int div=a/b;
		System.out.println("Division of "+" " +a+ " and " +b+ " is " +div);
	}
}
