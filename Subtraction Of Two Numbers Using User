import java.util.Scanner;
public class Subtract
{
	public static void main(String []args)
	{
		Scanner user=new Scanner(System.in);
		System.out.println("Enter two numbers");
		int a=user.nextInt();
		int b=user.nextInt();
		int s=a-b;
		System.out.println("Subtraction of "+" " +a+ " and " +b+ " is " +s);
	}
}
