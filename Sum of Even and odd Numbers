import java.util.Scanner;
     class CEvenOdd
	 {
		 public static void main(String[] args)
		 {
			 Scanner sc=new Scanner(System.in);
			 System.out.println("How much Number you want to Check ");
			 int n=sc.nextInt();
			 
			 int c=0, d=0;
			 
			 for(int i=0;i<=n;i++)
			 {
				 
				 if(i%2==0)
				 {
					  c=c+i;
				 }
				  else
				  {
					  d=d+i;
				  }
				  
					
			 }
			 System.out.println("Sum of Even No. "+c);
			 System.out.println("Sum of Odd No. "+d);
		 }
	 }
